{
    "name": "Inventory Source Scraper",
    "version": "1.0.0",
    "description": "This chrome extension works to scrape products from app.inventorysource.com",
    "manifest_version": 2,
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "browser_action": {
        "default_icon": "images/icon-48.png",
        "default_title": "Inventory Source Scraper",
        "default_popup": "html/popup.html"
    },
    "background": {
        "scripts": [
            "js/background.js"
        ],
        "persistent": true
    },
    "permissions": [
        "storage",
        "http://127.0.0.1:5000/*"
    ]
}
  